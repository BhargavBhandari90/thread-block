{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "buntywp/thread-item",
	"version": "1.0.0",
	"title": "Thread",
	"category": "widgets",
	"icon": "list-view",
	"description": "Thread Item.",
	"example": {},
	"supports": {
		"html": false,
		"shadow": true,
		"spacing": {
			"margin": true,
			"padding": true
		},
		"__experimentalBorder": {
			"radius": true,
			"color": true,
			"width": true,
			"style": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true
			}
		},
		"color": {
			"background": true,
			"text": true,
			"heading": true
		}
	},
	"attributes": {
		"sections": {
			"type": "array",
			"default": []
		},
		"threadLineColor": {
			"type": "string"
		},
		"threadLineWidth": {
			"type": "string"
		},
		"threadLineType": {
			"type": "string"
		}
	},
	"textdomain": "thread-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
